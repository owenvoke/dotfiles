# Shortcuts
alias c="clear"
alias cls="clear"
alias get-ip="echo $(curl -s https://canihazip.com/s)"

# Laravel
alias a="php artisan"
alias artisan="php artisan"

# PHP
alias cfresh="rm -rf vendor/ composer.lock && composer i"

# Functions
function proj() {
  cd "$HOME/projects/$1" || return
  printf "\033c"
}
